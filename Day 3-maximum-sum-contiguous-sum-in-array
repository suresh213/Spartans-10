#include <bits/stdc++.h> 
using namespace std; 

int main() 
{ 
    int i,n,sum=0,max=0;
    cin>>n;
    int a[n];
    for(i=0;i<n;i++)
    {
    	cin>>a[i];
	}
	for(i=0;i<n;i++)
    {
        sum+=a[i];
        if(sum>max)
        {
        	max=sum;
		}
		else if(sum<0)
		{
			sum=0;
		}
	}
	cout<<max;
    return 0; 
} 
